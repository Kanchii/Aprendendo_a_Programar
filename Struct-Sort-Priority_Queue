typedef vector<vpii> vvpii;

struct Order {
	bool operator()(pii const &a, pii const &b) {       //Struct para ordenar uma priority_queue de pair<int, int> de maneira
		if(a.first == b.first){                           //que o .first do pair Ã© ordenado pelo menor assim como o .second
			return a.second > b.second;
		}
		return a.first > b.first;
	}
};

